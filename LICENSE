  # Triggers the workflow on push or pull request events but only for the "paradice" branch
  push:
    branches: [ "Paradise" ]
  pull_request:
    branches: [ "paradice" ]
  # Allows you to run this workflow manually from the Actions tab
  workflow_dispatch:
